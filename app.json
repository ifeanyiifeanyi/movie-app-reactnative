    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/1adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ifeanyi22.Sabicatechism"
    },
    "web": {
      "favicon": "./assets/1adaptive-icon.png"
    }
