{
  "expo": {
    "name": "Sabi catechism",
    "slug": "Sabi-catechism",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/1adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/1adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
