    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/1adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.ifeanyi22.Sabicatechism"
    },
    "web": {
      "favicon": "./assets/1adaptive-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0103c8bb-f0ad-4a53-ab3c-8d4ab1035863"
      }
    }
  }
}
